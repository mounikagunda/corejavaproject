package packgae1;

public class Employee 
{private String  eid;
private String  ename;
private int  esal;
private String  eloc;
private int  eexp;

public String getEid() {
	return eid;
}
public void setEid(String eid) {
	this.eid = eid;
}
public String getEname() {
	return ename;
}
public void setEname(String ename) {
	this.ename = ename;
}
public int getEsal() {
	return esal;
}
public void setEsal(int esal) {
	this.esal = esal;
}
public String getEloc() {
	return eloc;
}
public void setEloc(String eloc) {
	this.eloc = eloc;
}
public int getEexp() {
	return eexp;
}
public void setEexp(int eexp) {
	this.eexp = eexp;
}


}
